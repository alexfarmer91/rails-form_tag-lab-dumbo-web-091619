<h3>Student Form</h3>

<form action="<%= students_path %>" method="POST">
  <label>First name:</label><br>
  <input type="text" id="student_first_name" name="student[first_name]"><br>

  <label>Last name:</label><br>
  <textarea id="student_last_name" name="student[last_name]"></textarea><br>

  <input type="submit" value="Submit Student">
</form>

<%= params.inspect %>